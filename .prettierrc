{
  "plugins": ["prettier-plugin-blade", "@prettier/plugin-php"],
  "overrides": [
    {
      "files": [
        "*.blade.php"
      ],
      "options": {
        "parser": "blade"
      }
    },
    {
      "files": [
        "*.php"
      ],
      "options": {
        "parser": "php"
      }
    }
  ]
}